{
  "name": "ttm-konnect-bind",
  "version": "1.0.1",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "bundle:web": "npx expo export --platform web",
    "serve:web": "npx serve dist",
    "prebuild:clean": "npx expo prebuild --clean",
    "compile": "tsc --noEmit -p . --pretty",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "align-deps": "npx expo install --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:maestro": "maestro test -e MAESTRO_APP_ID=com.ttm.ttmkonnect .maestro/flows",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "build:ios:sim": "eas build --profile development --platform ios --local",
    "build:ios:dev": "eas build --profile development:device --platform ios --local",
    "build:ios:preview": "eas build --profile preview --platform ios --local",
    "build:ios:prod": "eas build --profile production --platform ios --local",
    "build:android:sim": "eas build --profile development --platform android --local",
    "build:android:dev": "eas build --profile development:device --platform android --local",
    "build:android:preview": "eas build --profile preview --platform android --local",
    "build:android:prod": "node scripts/increment-version-code.js && eas build --profile production --platform android --local",
    "build:android:prod:apk": "eas build --platform android --profile production-apk --local",
    "increment:version": "node scripts/increment-version-code.js",
    "build:android:prod:no-increment": "eas build --profile production --platform android --local",
    "build:android:prod:obfuscated": "ENABLE_OBFUSCATION=true NODE_ENV=production eas build --profile production --platform android --local",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:dependencies": "node scripts/analyze-dependencies.js",
    "analyze:native-libs": "bash scripts/analyze-native-libs.sh",
    "optimize:assets": "node scripts/optimize-assets.js",
    "optimize:fonts": "node scripts/optimize-fonts.js",
    "optimize:bundle": "node scripts/optimize-bundle.js",
    "evaluate:alternatives": "node scripts/evaluate-dependency-alternatives.js",
    "track:size": "node scripts/size-tracker.js",
    "track:size:baseline": "node scripts/size-tracker.js --set-baseline",
    "test:security": "node scripts/test-security.js",
    "extract:certs": "bash scripts/extract-certificate-hashes.sh",
    "verify:16kb": "bash scripts/fix-16kb.sh",
    "prebuild:check": "bash scripts/pre-build-check.sh",
    "prebuild:fix": "bash scripts/auto-fix-build-issues.sh",
    "prebuild": "npm run prebuild:fix && npm run prebuild:check",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:build:release": "cd android && ./gradlew bundleRelease && cd ..",
    "android:build:apk": "cd android && ./gradlew assembleRelease && cd ..",
    "android:build": "npm run prebuild && npm run android:clean && npm run android:build:release",
    "android:build:apk:full": "npm run prebuild && npm run android:clean && npm run android:build:apk",
    "android:check:aab": "ls -lh android/app/build/outputs/bundle/release/*.aab 2>/dev/null || echo 'No AAB file found. Run: npm run android:build:release'",
    "android:check:apk": "ls -lh android/app/build/outputs/apk/release/*.apk 2>/dev/null || echo 'No APK file found. Run: npm run android:build:apk'",
    "android:install": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "android:logcat": "adb logcat | grep -i '16kb\\|page.*size\\|ttmkonnect'",
    "build:full": "npm run prebuild && npm run android:clean && npm run android:build:release && npm run android:check:aab",
    "build": "npm run prebuild && npm run build:android:prod:no-increment",
    "build:prod": "npm run prebuild && npm run build:android:prod"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-native-firebase/analytics": "^20.5.0",
    "@react-native-firebase/app": "^20.5.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@tanstack/react-query": "^5.89.0",
    "apisauce": "3.1.1",
    "date-fns": "^4.1.0",
    "expo": "^53.0.15",
    "expo-asset": "~11.1.7",
    "expo-background-fetch": "~12.0.0",
    "expo-battery": "~9.1.4",
    "expo-build-properties": "~0.14.6",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.1",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.1",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.4",
    "expo-localization": "~16.1.5",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.6",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.9",
    "expo-system-ui": "~5.0.9",
    "expo-task-manager": "~12.0.6",
    "i18next": "25.4.2",
    "intl-pluralrules": "^2.0.1",
    "lottie-react-native": "^7.3.4",
    "lucide-react-native": "^0.544.0",
    "react": "19.0.0",
    "react-i18next": "15.7.2",
    "react-native": "0.79.6",
    "react-native-blob-util": "^0.23.2",
    "react-native-chart-kit": "^6.12.0",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-freshchat-sdk": "^4.7.8",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-controller": "^1.12.7",
    "react-native-mmkv": "^3.3.3",
    "react-native-paper": "^5.14.5",
    "react-native-pdf": "^7.0.3",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~3.19.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "^13.16.0",
    "realm": "^20.2.0",
    "victory": "^37.3.6",
    "victory-native": "^41.20.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-jest": "^29.2.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-reactotron": "^0.1.2",
    "imagemin": "^9.0.1",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "javascript-obfuscator": "^4.1.1",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.7",
    "ora": "^6.3.1",
    "prettier": "^3.3.3",
    "react-native-bundle-visualizer": "^3.1.3",
    "react-test-renderer": "19.0.0",
    "reactotron-core-client": "^2.9.4",
    "reactotron-react-js": "^3.3.11",
    "reactotron-react-native": "^5.0.5",
    "reactotron-react-native-mmkv": "^0.2.6",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
